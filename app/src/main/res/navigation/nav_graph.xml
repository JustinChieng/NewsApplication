<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/newsFeedFragment4">


    <fragment
        android:id="@+id/newsFeedFragment4"
        android:name="com.justin.ui.fragments.NewsFeedFragment"
        android:label="NewsFeedFragment"
        tools:layout="@layout/news_feed_list_items">


        <action
            android:id="@+id/action_newsFeedFragment4_to_detailsFragment"
            app:destination="@id/detailsFragment" />

    </fragment>
    <fragment
        android:id="@+id/detailsFragment"
        android:name="com.justin.ui.fragments.DetailsFragment"
        android:label="DetailsFragment"
        tools:layout="@layout/fragment_details">
        <argument
            android:name="path"
            app:argType="string" />
    </fragment>


</navigation>